# Example environment variables for resume-backend
# Copy this file to `.env` and fill the values before running

# Port the server listens on
PORT=5000

# MongoDB connection string
# e.g. mongodb://localhost:27017/resume-db
MONGO_URI=

# Gemini / Google Generative API key (GEMINI API)
# Set this to your provider key if you want optimize to work.
GEMINI_API_KEY=
